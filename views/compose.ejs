<%- include("./partials/header") %>
    <link rel="stylesheet" href="./css/styles1.css">

    <body>
        <h1>
            <%= pageTitle %>
        </h1>

        <form action="/compose" method="post">
            <div class="form-group">
                <div class="form-group">
                    <label for="postTitle">Title</label><input class="form-control" name="postTitle"
                        type="text" autocomplete="off">
                </div>
                <div class="form-group">
                    <label for="postBody">Content</label>
                    <textarea name="postBody"class="form-control" autocomplete="off">
                    </textarea>

                </div>
                <div class="form-group">
                    <label for="submitbtn"></label>
                    <button class="btn btn-primary" name="submitbtn" type="submit">Publish</button>
                </div>
            </div>
        </form>
    </body>
    <%- include("./partials/footer") %>